PROCEDURE Dot product(v1,v2 : ARRAY_OF INTEGER [] , var vec : INTEGER;)
VAR
    i : INTEGER;
BEGIN
    vec := 0;
    i:= 0;
    IF (v1.length<>v2.length) THEN
        write ("Ne pouves pas calculer les vecteurs avec des tailles diff√©rentes")
        vec = Nnbre;
    END_IF
    WHILE (i<v1.length) DO
        vec = vec+v1[i] * v2[i];
    END_WHILE
END
